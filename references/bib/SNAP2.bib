  shorttitle = {Impact of Follow-up Consultations for {{ICU}} Survivors on Post-{{ICU}} Syndrome},
  author = {Jensen, J. F. and Thomsen, T. and Overgaard, D. and Bestle, M. H. and Christensen, D. and Egerod, I.},
