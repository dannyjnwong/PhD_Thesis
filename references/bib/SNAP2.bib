  title = {Factors Predicting Hospital Length-of-Stay and Readmission after Colorectal Resection: A Population-Based Study of Elective and Emergency Admissions},
  shorttitle = {Factors Predicting Hospital Length-of-Stay and Readmission after Colorectal Resection},
  author = {Kelly, Maria and Sharp, Linda and Dwane, Fiona and Kelleher, Tracy and Comber, Harry},
  year = {2012},
  month = mar,
