  author = {J{\o}rgensen, Christoffer C. and Petersen, Morten Aa and Kehlet, Henrik and Group, on behalf of the Lundbeck Foundation Centre for Fast-Track Hip {and} Knee Replacement Collaborative and Soeballe, Kjeld and Hansen, Torben B. and Husted, Henrik and Laursen, Mogens B. and Hansen, Lars T. and {Kj{\ae}rsgaard-Andersen}, Per and Solgaard, S{\o}ren},
