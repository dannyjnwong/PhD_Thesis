@article{bougeard_survey_2017,
  title = {A Survey of {{UK}} Peri-Operative Medicine: Pre-Operative Care},
  shorttitle = {A Survey of {{UK}} Peri-Operative Medicine},
  author = {Bougeard, A.-M. and Brent, A. and Swart, M. and Snowden, C.},
