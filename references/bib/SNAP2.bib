}

@article{prytherch_possum_1998,
  title = {{{POSSUM}} and {{Portsmouth POSSUM}} for Predicting Mortality},
  author = {Prytherch, D. R. and Whiteley, M. S. and Higgins, B. and Weaver, P. C. and Prout, W. G. and Powell, S. J.},
