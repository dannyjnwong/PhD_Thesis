  pages = {819--824},
  issn = {1365-2044},
  doi = {10.1111/anae.14269},
  abstract = {Postoperative hospital stay is longer for frail, older patients, who are more likely to experience prolonged postoperative morbidity and reduced long-term survival. We recorded in-hospital mortality, morbidity and length of stay for 164 patients aged at least 65 years after unscheduled surgery. We evaluated pre-operative frailty with the 7-point Clinical Frailty Scale: 81 patients were `not vulnerable' (frailty score 1\textendash 3) and 83 were `vulnerable or frail' (frailty score 4), with mean (SD) ages of 74.7 (7.5) years vs. 79.4 (8.3) years, respectively, p {$<$} 0.001. Within 30 postoperative days 8/164 (5\%) patients died, all with frailty scores 4, p = 0.007. Postoperative morbidity was less frequent in patients categorised as `not vulnerable' on four out of the six days it was measured (days 3, 5, 8, 14, 23, 28). Median (IQR [range]) postoperative stay was 9 (6\textendash 18 [2\textendash 221]) days for patients with frailty scores 1\textendash 3, and 22 (12\textendash 33 [2\textendash 270]) days for patients with score 4, p {$<$} 0.001. Four variables independently associated with hospital discharge, hazard ratio (95\%CI): E-POSSUM, 0.74 (0.60\textendash 0.92), p = 0.007; ASA 2, 0.35 (0.13\textendash 0.98), p = 0.046, ASA 3, 0.17 (0.06\textendash 0.47), p = 0.001 and ASA 4/5, 0.08 (0.02\textendash 0.28), p {$<$} 0.001; operative severity `major +', 0.69 (0.41\textendash 1.08), p = 0.10 and the Surgical Outcome Risk Tool, 7.75 (0.81\textendash 74.40), p = 0.08.},
  copyright = {\textcopyright{} 2018 The Association of Anaesthetists of Great Britain and Ireland},
  file = {C\:\\Users\\Danny\\Zotero\\storage\\IQTRW7WI\\McGuckin et al. - 2018 - The association of peri-operative scores, includin.pdf;C\:\\Users\\Danny\\Zotero\\storage\\CVHDGH24\\anae.html},
  journal = {Anaesthesia},
  keywords = {ageing,ASA physical status,geriatric anaesthesia,quality measures: patient care},
  language = {en},
  number = {7}
}

