@article{schneider_prognostic_2009,
  title = {Prognostic {{Factors}} in {{Critically Ill Patients Suffering}} from {{Secondary Peritonitis}}: {{A Retrospective}}, {{Observational}}, {{Survival Time Analysis}}},
  shorttitle = {Prognostic {{Factors}} in {{Critically Ill Patients Suffering}} from {{Secondary Peritonitis}}},
  author = {Schneider, Christian P. and Seyboth, Carol and Vilsmaier, Markus and K{\"u}chenhoff, Helmut and Hofner, Benjamin and Jauch, Karl-Walter and Hartl, Wolfgang H.},
  year = {2009},
