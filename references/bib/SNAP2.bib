@article{harris_impact_2018,
  title = {Impact on Mortality of Prompt Admission to Critical Care for Deteriorating Ward Patients: An Instrumental Variable Analysis Using Critical Care Bed Strain},
  shorttitle = {Impact on Mortality of Prompt Admission to Critical Care for Deteriorating Ward Patients},
  author = {Harris, Steve and Singer, Mervyn and Sanderson, Colin and Grieve, Richard and Harrison, David and Rowan, Kathryn},
