
@article{abbott_frequency_2017,
  title = {Frequency of Surgical Treatment and Related Hospital Procedures in the {{UK}}: A National Ecological Study Using Hospital Episode Statistics},
  shorttitle = {Frequency of Surgical Treatment and Related Hospital Procedures in the {{UK}}},
  author = {Abbott, T. E. F. and Fowler, A. J. and Dobbs, T. D. and Harrison, E. M. and Gillies, M. A. and Pearse, R. M.},
  year = {2017},
  month = aug,
  volume = {119},
  pages = {249--257},
  issn = {0007-0912},
  doi = {10.1093/bja/aex137},
  abstract = {Background: Despite evidence of high activity, the number of surgical procedures performed in UK hospitals, their cost and subsequent mortality remain unclear.Methods: Time-trend ecological study using hospital episode data from England, Scotland, Wales and Northern Ireland. The primary outcome was the number of in-hospital procedures, grouped using three increasingly specific categories of surgery. Secondary outcomes were all-cause mortality, length of hospital stay and healthcare costs according to standard National Health Service tariffs.Results: Between April 1, 2009 and March 31, 2014, 39 631 801 surgical patient episodes were recorded. There was an annual average of 7 926 360 procedures (inclusive category), 5 104 165 procedures (intermediate category) and 1 526 421 procedures (restrictive category). This equates to 12 537, 8073 and 2414 procedures per 100 000 population per year, respectively. On average there were 85 181 deaths (1.1\%) within 30 days of a procedure each year, rising to 178 040 deaths (2.3\%) after 90 days. Approximately 62.8\% of all procedures were day cases. Median length of stay for in-patient procedures was 1.7 (1.3\textendash 2.0) days. The total cost of surgery over the 5 yr period was \textsterling 54.6 billion (\$104.4 billion), representing an average annual cost of \textsterling 10.9 billion (inclusive), \textsterling 9.5 billion (intermediate) and \textsterling 5.6 billion (restrictive). For each category, the number of procedures increased each year, while mortality decreased. One-third of all mortalities in national death registers occurred within 90 days of a procedure (inclusive category).Conclusions: The number of surgical procedures in the UK varies widely according to definition. The number of procedures is slowly increasing whilst the number of deaths is decreasing.},
  file = {C\:\\Users\\Danny\\Zotero\\storage\\7WVARN8H\\AbbottFowler.FrequencyOfSurgery.Supplement.R2_aex137.docx;C\:\\Users\\Danny\\Zotero\\storage\\QSDJ77DZ\\Abbott et al. - 2017 - Frequency of surgical treatment and related hospit.pdf;C\:\\Users\\Danny\\Zotero\\storage\\3K7B2RMS\\4049141.html},
  journal = {British Journal of Anaesthesia},
  number = {2}
}

@article{abbott_prospective_2018,
  title = {A {{Prospective International Multicentre Cohort Study}} of {{Intraoperative Heart Rate}} and {{Systolic Blood Pressure}} and {{Myocardial Injury After Noncardiac Surgery}}: {{Results}} of the {{VISION Study}}},
  shorttitle = {A {{Prospective International Multicentre Cohort Study}} of {{Intraoperative Heart Rate}} and {{Systolic Blood Pressure}} and {{Myocardial Injury After Noncardiac Surgery}}},
  author = {Abbott, Tom E. F. and Pearse, Rupert M. and Archbold, R. Andrew and Ahmad, Tahania and Niebrzegowska, Edyta and Wragg, Andrew and Rodseth, Reitze N. and Devereaux, Philip J. and Ackland, Gareth L.},
  year = {2018},
  month = jun,
  volume = {126},
  pages = {1936--1945},
  issn = {0003-2999},
  doi = {10.1213/ANE.0000000000002560},
  abstract = {BACKGROUND: The association between intraoperative cardiovascular changes and perioperative myocardial injury has chiefly focused on hypotension during noncardiac surgery. However, the relative influence of blood pressure and heart rate (HR) remains unclear. We investigated both individual and codependent relationships among intraoperative HR, systolic blood pressure (SBP), and myocardial injury after noncardiac surgery (MINS). METHODS: Secondary analysis of the Vascular Events in Noncardiac Surgery Cohort Evaluation (VISION) study, a prospective international cohort study of noncardiac surgical patients. Multivariable logistic regression analysis tested for associations between intraoperative HR and/or SBP and MINS, defined by an elevated serum troponin T adjudicated as due to an ischemic etiology, within 30 days after surgery. Predefined thresholds for intraoperative HR and SBP were: maximum HR {$>$}100 beats or minimum HR {$<$}55 beats per minute (bpm); maximum SBP {$>$}160 mm Hg or minimum SBP {$<$}100 mm Hg. Secondary outcomes were myocardial infarction and mortality within 30 days after surgery. RESULTS: After excluding missing data, 1197 of 15,109 patients (7.9\%) sustained MINS, 454 of 16,031 (2.8\%) sustained myocardial infarction, and 315 of 16,061 patients (2.0\%) died within 30 days after surgery. Maximum intraoperative HR {$>$}100 bpm was associated with MINS (odds ratio [OR], 1.27 [1.07\textendash 1.50]; P {$<$} .01), myocardial infarction (OR, 1.34 [1.05\textendash 1.70]; P = .02), and mortality (OR, 2.65 [2.06\textendash 3.41]; P {$<$} .01). Minimum SBP {$<$}100 mm Hg was associated with MINS (OR, 1.21 [1.05\textendash 1.39]; P = .01) and mortality (OR, 1.81 [1.39\textendash 2.37]; P {$<$} .01), but not myocardial infarction (OR, 1.21 [0.98\textendash 1.49]; P = .07). Maximum SBP {$>$}160 mm Hg was associated with MINS (OR, 1.16 [1.01\textendash 1.34]; P = .04) and myocardial infarction (OR, 1.34 [1.09\textendash 1.64]; P = .01) but, paradoxically, reduced mortality (OR, 0.76 [0.58\textendash 0.99]; P = .04). Minimum HR {$<$}55 bpm was associated with reduced MINS (OR, 0.70 [0.59\textendash 0.82]; P {$<$} .01), myocardial infarction (OR, 0.75 [0.58\textendash 0.97]; P = .03), and mortality (OR, 0.58 [0.41\textendash 0.81]; P {$<$} .01). Minimum SBP {$<$}100 mm Hg with maximum HR {$>$}100 bpm was more strongly associated with MINS (OR, 1.42 [1.15\textendash 1.76]; P {$<$} .01) compared with minimum SBP {$<$}100 mm Hg alone (OR, 1.20 [1.03\textendash 1.40]; P = .02). CONCLUSIONS: Intraoperative tachycardia and hypotension are associated with MINS. Further interventional research targeting HR/blood pressure is needed to define the optimum strategy to reduce MINS.},
  file = {C\:\\Users\\Danny\\Zotero\\storage\\7N533QTD\\Abbott et al. - 2018 - A Prospective International Multicentre Cohort Stu.pdf},
  journal = {Anesthesia and Analgesia},
  number = {6},
  pmcid = {PMC5815500},
  pmid = {29077608}
}

@article{abeeleh_reasons_2017,
  title = {Reasons for Operation Cancellations at a Teaching Hospital: Prioritizing Areas of Improvement},
  shorttitle = {Reasons for Operation Cancellations at a Teaching Hospital},
  author = {Abeeleh, Mahmoud Abu and Tareef, Tareq M. and Hani, Amjad Bani and Albsoul, Nader and Samarah, Omar Q. and ElMohtaseb, M. S. and Alshehabat, Musa and Ismail, Zuhair Bani and Alnoubani, Omar and Obeidat, Salameh S. and Halawa, Sami Abu},
  year = {2017},
  month = aug,
  volume = {93},
  pages = {65--69},
  issn = {2288-6575},
  doi = {10.4174/astr.2017.93.2.65},
  abstract = {PURPOSE: To report rates of and reasons for operation cancellation, and to prioritize areas of improvement. METHODS: Retrospective data were extracted from the monthly reports of cancelled listed operations. Data on 14 theatres were collected by the office of quality assurance at Jordan University Hospital from August 2012 to April 2016. Rates and reasons for operation cancellation were investigated. A Pareto chart was constructed to identify the reasons of highest priority. RESULTS: During the period of study, 6,431 cases (9.31\%) were cancelled out of 69,066 listed cases. Patient no-shows accounted for 62.52\% of cancellations. A Pareto analysis showed that around 80\% of the known reasons for cancellation after admission were due to a lack of surgical theatre time (30\%), incomplete preoperative assessment (21\%), upper respiratory tract infection (19\%), and high blood pressure (13\%). CONCLUSION: This study identified the most common reasons for operation cancellation at a teaching hospital. Potential avoidable root causes and recommended interventions were suggested accordingly. Future research, available resources, hospital policies, and strategic measures directed to tackle these reasons should take priority.},
  journal = {Annals of Surgical Treatment and Research},
  keywords = {Operation cancellation,Quality improvement},
  language = {eng},
  number = {2},
  pmcid = {PMC5566748},
  pmid = {28835881}
}

